package eu.stefaner.elasticlists.data {		public class GeoFacet extends Facet {				public function GeoFacet(name:String = "", label:String = null) {			super(name, label);		}		override public function createFacetValue(name:String):FacetValue {			if(hasFacetValueWithName(name)) {				throw new Error("Could not create FacetValue - Name already taken!");				return;			}			var f:GeoFacetValue = new GeoFacetValue(name);			addFacetValue(f);			return f;		};				// is managed directly.		override public function updateContentItemFilter() : Filter {			return filter;		}	}}